<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" >
        <shape android:thickness="3dp">
            <corners android:radius="15dip" />
            <gradient android:startColor="#e91e63" android:centerColor="#e91e63"
                android:centerY="0.75" android:endColor="#ffffff" android:angle="90" />
            <stroke android:width="5dp" android:color="#eeeeee" />
        </shape>
    </item>
    <item android:id="@android:id/progress" >
        <clip>
            <shape>
                <corners android:radius="15dip" />
                <gradient android:startColor="#0288d1" android:endColor="#0288d1"
                    android:angle="90" />
                <stroke android:width="2dp" android:color="#eeeeee" />
            </shape>
        </clip>
    </item>
</layer-list>